import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
    int i,j;
 int n=sc.nextInt();
int a[]=new int[n];
int sum[]=new int[n];

for(i=0;i<n;i++) 
{a[i]=sc.nextInt();
 sum[i]=0;
}

 for(i=0;i<n;i++)
 {  for(j=0;j<n;j++)
 {if(i!=j) sum[i]+=a[j];}
 }
 for(i=0;i<n;i++) System.out.println(sum[i]);
	}
}
